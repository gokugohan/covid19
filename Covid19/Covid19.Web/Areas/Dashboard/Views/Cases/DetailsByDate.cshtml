@model IEnumerable<Covid19.Entities.Models.Graph>
@{
    ViewData["Title"] = "DetailsByDate";
}

    <div class="container">
        <div class="table-responsive">
            <h1 class="mt-4">Data for @ViewData["datetime"]</h1>

            <table class="table table-hover">
                <thead>
                    <tr>
                        <th>Kazu</th>
                        <th>Total</th>
                        <th></th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>@item.Kazu</td>
                            <td>@item.Total</td>
                            <td><a asp-action="Edit" asp-route-id="@item.Id" class="btn btn-sm btn-info"><i class="fa fa-edit"></i></a></td>
                        </tr>
                    }
                </tbody>
            </table>
            <a asp-action="Index">Back to List</a>
        </div>
    </div>