<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Covid-19</title>
    <meta name="author" content="Helder Chebre">
    <meta name="description" content="Página disponibilizada atu hare dadus konaba corona virus (covid-19). Dados em tempo real foti husi worldometers.com no corona-stats">
    <meta name="keywords" content="corona, corona-virus, coronavirus, covid, covid-19, corona realtime, corona-map">

    <!-- Open graph tags -->
    <meta property="og:title" content="Página disponibilizada atu hare dadus monitora corona virus (covid-19). Fonte husi ministério da saúde de RDTL, worldometers.com no corona-stats">
    <meta property="og:type" content="map">
    <meta property="og:url" content="http://chebre.net/images/preview.png">
    <meta property="og:image" content="http://chebre.net/images/preview.png">
    <meta property="og:description"
          content="Página disponibilizada atu hare dadus konaba monitora corona virus (covid-19).  Fonte husi ministério da saúde de RDTL, worldometers.com no corona-stats">
    <meta property="og:site_name" content="HC - COVID-19">

    <meta property="article:author" content="https://www.facebook.com/helderchebre">

    <!-- Favicons -->
    <link href="~/favicon.ico" rel="icon">

    <!--Import Google Icon Font-->
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link href="~/lib/materialize/css/materialize.min.css" rel="stylesheet" />
    <link href="~/css/jquery.dataTables.min.css" rel="stylesheet" />

    <link href="~/css/toastr.min.css" rel="stylesheet" />
    <link href="~/css/site.css" rel="stylesheet" />
    @RenderSection("css", false)

</head>
<body>
    @RenderSection("header", false)
    <main role="main">
        @RenderBody()
    </main>

    <!-- ======= Footer ======= -->
    <footer class="page-footer">
        <div class="footer-copyright">
            <div class="container">
                © @DateTime.Now.Year All Right Reserved
                <a class="grey-text text-lighten-4 right" href="#!">More Links</a>
            </div>
        </div>
    </footer><!-- End Footer -->
    <!-- Modal -->
    <div class="modal" id="modal-account">
        <div class="modal-content">
            <h5 class="modal-title"></h5>
            <div class="modal-body"></div>
        </div>
    </div>

    <a href="#" class="back-to-top"><i class="icofont-simple-up"></i></a>
    <input type="hidden" id="message" value="@TempData["message"]" />


    <script src="~/lib/jquery/dist/jquery.min.js"></script>

    <script src="~/lib/materialize/js/materialize.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@9"></script>
    <script src="~/js/highcharts.js"></script>

    <script src="~/js/jquery.dataTables.min.js"></script>
    <script src="~/js/toastr.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>


    <script>
        $(document).ready(function () {
            var message = $("#message").val();
            if (message != "") {
                Swal.fire({
                    icon: 'error',
                    title: 'Oops...',
                    text: message,
                })
            }

        });
    </script>
    @RenderSection("Scripts", required: false)

    <!-- Default Statcounter code for Covid 19 Project http://covid19.chebre.net/
    -->
    <script type="text/javascript">
        var sc_project = 12233542;
        var sc_invisible = 1;
        var sc_security = "00a29427";
        var sc_https = 1;
    </script>
    <script type="text/javascript" src="https://www.statcounter.com/counter/counter.js" async></script>
    <!-- End of Statcounter Code -->

</body>
</html>
